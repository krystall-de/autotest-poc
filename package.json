  "license": "MIT",
  "scripts": {
    "docker:up": "bash -c 'docker compose -p ${PROJECT_NAME} -f docker/docker-compose.${ENV,,}.yml up -d'",
    "docker:down": "bash -c 'docker compose -p ${PROJECT_NAME} -f docker/docker-compose.${ENV,,}.yml down'",
    "db:gen-client": "yarn prisma generate",
    "db:gen-migrate": "bash -c 'NAME=${*:-migration}; yarn prisma migrate dev --name \"$NAME\"' --",
    "db:migrate": "yarn prisma migrate deploy",
    "db:seed": "yarn prisma db seed",
    "db:reset": "yarn prisma migrate reset",
    "postinstall": "yarn db:gen-client"
  },
  "private": true,
